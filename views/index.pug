extends layout

block content
    .container
        .row
            .col-md-3
                h1= title
                ul.list-group
                    for i in [1,2,3,4]
                        li.list-group-item= i

                button.btn.btn-primary(onclick="alert(number())")
                    i.fa.fa-fw.fa-list

    script.
        function number() {
            return Math.random() * 150;
        }